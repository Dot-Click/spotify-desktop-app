    },
    "mac": {
      "target": "dmg",
      "icon": "./src/assets/spotify.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "./src/assets/spotify.png"
    }
  },
  "proxy": "https://spotify-json.vercel.app",
